<?xml version="1.0" ?>
<Manifest 
    xmlns="http://schemas.microsoft.com/xw/2004/10/manifest.html"
    xmlns:dssp="http://schemas.microsoft.com/xw/2004/10/dssp.html"
    >
    <CreateServiceList>

    <ServiceRecordType>
        <dssp:Contract>http://www.promrds.com/contracts/2008/01/mazesimulator.html</dssp:Contract>
        <dssp:PartnerList>
          <!--Initial config file -->
          <dssp:Partner>
            <dssp:Service>MazeSimulator.Config.xml</dssp:Service>
            <dssp:Name>dssp:StateService</dssp:Name>
          </dssp:Partner>
        </dssp:PartnerList>
    </ServiceRecordType>
